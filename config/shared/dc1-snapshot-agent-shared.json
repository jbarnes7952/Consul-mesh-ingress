{
  "data_dir": "/data/",
  "snapshot_agent": {
    "http_addr": "10.10.0.2:8500",
    "datacenter": "dc1",
    "log": {
      "level": "TRACE",
      "enable_syslog": false,
      "syslog_facility": "LOCAL0"
    },
    "snapshot": {
      "interval": "1m",
      "retain": 30,
      "stale": false,
      "service": "consul-snapshot",
      "deregister_after": "72h",
      "lock_key": "consul-snapshot/lock",
      "max_failures": 3,
      "local_scratch_path": ""
    },
    "local_storage": {
      "path": "."
    }
  } 
}
