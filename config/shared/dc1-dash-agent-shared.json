{
  "acl": {
 	  "tokens": {
             "default": "66008c6d-62e6-74dc-d597-32c5f1dd10a4"
	  },
  "enabled": true,
    "default_policy": "deny",
    "down_policy": "extend-cache",
    "enable_token_persistence": true
  },
  "data_dir": "/data/",
  "datacenter": "dc1",
  "server": false,
  "start_join": ["10.10.0.2", "10.10.0.3", "10.10.0.4"],
  "ui": true,
  "bind_addr": "0.0.0.0",
  "ports": {
      "dns": 53,
      "grpc": 8502
  },
  "dns_config": {
    "service_ttl": {
      "*": "10s"
    },
    "node_ttl": "10s"
  },
  "client_addr": "0.0.0.0",
  "service": {
    "name": "dashboard",
    "port": 9002,
    "connect": {
      "sidecar_service": {
        "proxy": {
      	  "upstreams": [
	    {
      	    "destination_name": "counting",
      	    "local_bind_port": 5000,
	    "datacenter": "dc2"
      	    }
	  ]
      	}
      }
    },
    "check": {
      "id": "dashboard-check",
      "http": "http://localhost:9002/health",
      "method": "GET",
      "interval": "1s",
      "timeout": "1s"
    }
  }
}
